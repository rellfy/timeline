{
    "compilerOptions": {
        "target": "es5",
        "lib": ["ES2018", "DOM"],
        "module": "commonjs",
        "jsx": "react",
        "moduleResolution": "node",
        "declaration": true,
        "allowJs": true,
        "noImplicitAny": true,
        "checkJs": true,
        "strict": true,
        "sourceMap": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "downlevelIteration": true,
        "types": ["node", "react"],
        "typeRoots": ["node_modules/@types"]
    }
}